<section class="list">
<h3>Users</h3>

<% @users.each do |user| %>    +<div class="row">
  <div class="row">    +  <% @users.each do |user| %>
    <div class="col-xs-1">   +    <div class="col-xs-8">
      <div class="round">    
        <%= image_tag user.avatar.url(:avatar) if user.avatar? %>    
        <%= image_tag "http://placehold.it/70x70&text=#{user.name}" unless user.avatar? %>   
      </div>   
    </div>   
   <div class="col-xs-5 middle">
      <%= user.email %>
    </div>
    <div class="col-xs-4">
      <% if current_user.is_admin? %>
        <%= link_to edit_user_path(user) do %>
          <paper-icon-button icon="create"></paper-icon-button>
        <% end %>
        <%= link_to user, method: :delete, data: { confirm: 'Are you sure?' } do %>
          <paper-icon-button icon="delete"></paper-icon-button>
        <% end %>
      <% end %>
    </div>
  <% end %>
</div>

<%= link_to 'New User', new_user_path %>
</section>